				</div>
			</div>
		</div>
	</div>
	<!-- end search area -->

	
	<!-- breadcrumb-section -->
	<div class="breadcrumb-section breadcrumb-bg">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 offset-lg-2 text-center">
					<div class="breadcrumb-text">
						<p>Your Loved Products</p>
						<h1>Wishlist</h1>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end breadcrumb section -->

	<section>

    <div class="container">
   
    <div class="orderlist">
   
         <table  class="table table-striped table-hover mt-5 bg-white">
  <thead>
    <tr>
     
     
      <th scope="col">Product</th>
      <th scope="col">Product Name</th>
      <th scope="col">Description</th>
      <th scope="col">Price</th>
      
     
    </tr>
  </thead>
  <tbody>
   {{#each products}}
    <tr>
      
    <td class="product-image"><img src="/product-images/{{this.product.image.[0]}}" alt=""></td>
    <td>{{this.product.name}}</td>
    <td>{{this.product.description}}</td>
	{{#if this.product.offer}}
    <td><span class="text-muted fs-6">per Kg</span>{{this.product.offerPrice}}/-</td>
	{{else}}
	<td><span class="text-muted fs-6">per Kg</span>{{this.product.price}}/-</td>
	{{/if}}
    <td> <a href="/login/viewProduct-wishlist/{{this.product._id}}"><button class="btn btn-success">View</button></a></td>
     <td><button class="btn-danger" onclick="removeFrmWishlist('{{this._id}}','{{this.product._id}}')">Remove</button></td>
    </tr>
   {{/each}}
  </tbody>
</table>
    </div>
    </div>
</section>

